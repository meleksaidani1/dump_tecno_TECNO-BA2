<?xml version="1.0" encoding="utf-8"?>
<EccTable>
    <!--
        Kuwait
        The attribute definition for tag EccEntry:
        - Ecc: the emergnecy number
        - Category: the service category
        - Condition: there are following values:
            - 0: ecc only when no sim
            - 1: ecc always
            - 2: MMI will show ecc but send to nw as normal call
    -->
    <EccEntry Ecc="112" Category="0" Condition="2" Name="Police/Ambulance/Fire Brigade"/>
    <EccEntry Ecc="152" Category="0" Condition="2" Name="Electricity &amp; Water"/>
    <EccEntry Ecc="135" Category="0" Condition="2" Name="Commerce and Industry"/>
    <EccEntry Ecc="139" Category="0" Condition="2" Name="Municipilaty"/>
    <EccEntry Ecc="151" Category="0" Condition="2" Name="Ministry of Health"/>
    <EccEntry Ecc="161" Category="0" Condition="2" Name="Airport"/>
</EccTable>
